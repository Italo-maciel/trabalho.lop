var x = 100;
var y = 100;
function setup() {
  createCanvas(500, 500);
}
function draw() {
  fill(0);
  rect(0, 0, 500, 500);
  fill('red');
  rect(170,200,70,70);
   fill(0, 225, 225);
  ellipse(300,240,55,55);
  function draw(){
  if (keyIsDown(LEFT_ARROW)) {
    x -= 5;
  }

  if (keyIsDown(RIGHT_ARROW)) {
    x += 5;
  }

  if (keyIsDown(UP_ARROW)) {
    y -= 5;
  }
